function plotRobots(RedRobots,BlueRobots,Ball)
%PLOTROBOTS

% Parameters
    robot_r = 0.15;
    ball_r = 0.05;
    
    for i=1:3
        draw_circle(RedRobots(i).X,RedRobots(i).Y,robot_r,'r',1);
        draw_circle(BlueRobots(i).X,BlueRobots(i).Y,robot_r,'b',1);
    end
    
    draw_circle(Ball.X,Ball.Y,ball_r,'g',1);

end

